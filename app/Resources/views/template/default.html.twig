   'bundles/empresa/web/admin/layout/scripts/'
   
            %}
                <script src = "{{ asset_url }}" ></script>
            {% endjavascripts %}


        <script>
                    jQuery(document).ready(function () {
                
                Metronic.init(); // init metronic core components
